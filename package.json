{
  "name": "curriculum-vitae",
  "version": "1.0.0",
  "private": true,
  "description": "curriculum-vitae",
  "author": "t-keshi",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "gen:theme-typings": "chakra-cli tokens ./src/components/theme/theme.ts --strict-component-types",
    "postinstall": "npm run gen:theme-typings & npm run clean",
    "build:pdf": "make & md-to-pdf ./src/posts/all.md --config-file ./pdf-configs/config.js",
    "copy": "cat ./src/posts/all.pdf > ./static/all.pdf"
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^2.0.4",
    "@chakra-ui/react": "^1.8.6",
