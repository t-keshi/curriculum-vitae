    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/noto-sans-jp": "^4.5.3",
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
    "axios": "^0.25.0",
    "dotenv": "^16.0.0",
    "emotion-theming": "^11.0.0",
    "framer-motion": "^6.2.6",
    "fs": "^0.0.1-security",
    "gatsby": "^4.7.1",
    "gatsby-plugin-canonical-urls": "^4.7.0",
    "gatsby-plugin-catch-links": "^4.8.0",
