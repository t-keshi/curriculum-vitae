    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-share": "^4.4.0",
    "simple-icons": "^7.10.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.8.2",
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "^1.0.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/js-search": "^1.4.0",
    "@types/mdx-js__react": "^1.5.5",
    "@types/prettier": "^2.4.1",
    "@types/react-helmet": "^6.1.5",
